<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nirajsarode.studentassitantapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Student Assistant"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WeekActivity" />
        <activity android:name=".DayDetail" />
        <activity android:name=".NoteActivityMain" />
        <activity android:name=".NoteActivity" />
        <activity android:name=".AttendanceMain"></activity>
    </application>

</manifest>